<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10.0" styleClass="root" stylesheets="@queue-style.css" VBox.vgrow="ALWAYS" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dev.monospace.plane_ahead.QueueController">
   <children>
      <Label prefWidth="300.0" text="Flight Schedule" />
      <AnchorPane prefWidth="300.0" styleClass="scroll-background" VBox.vgrow="ALWAYS">
         <children>
            <ScrollPane fitToWidth="true" vbarPolicy="ALWAYS" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <content>
                  <VBox fx:id="box" prefHeight="200.0" spacing="10.0">
                     <padding>
                        <Insets bottom="12.0" left="10.0" right="10.0" top="12.0" />
                     </padding>
                  </VBox>
               </content>
            </ScrollPane>
         </children>
      </AnchorPane>
      <Button alignment="CENTER" mnemonicParsing="false" styleClass="add-button" text="+ New">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
